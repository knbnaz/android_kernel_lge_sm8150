#
# LGE Board Configuration
#

CONFIG_MACH_LGE=y

#
# LGE Power Feature
#

CONFIG_LGE_PM=y
CONFIG_LGE_PM_PRM=y
CONFIG_LGE_PM_VENEER_PSY=y

#
# LGE Display
#

CONFIG_LGE_DISPLAY_COMMON=y
CONFIG_LGE_DISPLAY_DIMMING_BOOT_SUPPORT=y

#
# LGE Touchscreen
#

CONFIG_LGE_TOUCH_CORE=y
CONFIG_LGE_TOUCH_CORE=y
CONFIG_LGE_TOUCH_CORE_QCT=y
CONFIG_LGE_TOUCH_USE_PANEL_NOTIFY=y
CONFIG_SECURE_TOUCH=y

#
# LGE Filesystem Support
#

CONFIG_EXFAT_FS=y

#
# LGE USB Features
#

CONFIG_LGE_USB=y
CONFIG_LGE_USB_GADGET=y

#
# Multipath TCP
#

CONFIG_MPTCP=y

#
# Device Tree Overlay
#

CONFIG_BUILD_ARM64_DT_OVERLAY=y
CONFIG_BUILD_ARM64_KERNEL_COMPRESSION_GZIP=y
# CONFIG_BUILD_ARM64_UNCOMPRESSED_KERNEL is not set

#
# Miscellaneous
#

CONFIG_LGE_ONE_BINARY_SKU=y
